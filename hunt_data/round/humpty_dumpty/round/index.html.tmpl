{% include "round-head.html" %}

<div id="mh-controls">
  <a id="mh-back" href="{{ root }}">
    Back to ???
  </a>
  <a id="mh-answer" href="javascript:void(0);">
    Submit Answer
  </a>
</div>

<div id="mh-page">
  <div id="mh-banner">
    <div id="mh-title">
      {{ round.round.name }}
    </div>
  </div>

  <div id="mh-content">
    <ul>
      {% for puzzle in puzzles %}
      <li><a href="{{ root }}/puzzle/{{ puzzle.puzzle.url }}">{{ puzzle.puzzle.name }}</a>
	{% endfor %}
    </ul>
  </div>
</div>

{% include "round-tail.html" %}
