<!DOCTYPE html>
<html>
  <head>
    <title>Wonderland</title>
    <link rel="stylesheet" type="text/css" href="wonderland.css">
    <link href='https://fonts.googleapis.com/css?family=Parisienne|Alice' rel='stylesheet' type='text/css'>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
  </head>
  <body class="mh-fancy">
    <div id="mh-outer">
      <div id="mh-controls">
	<a id="mh-back" href="{{ root }}">
	  Back to Lobby 7
	</a>
	<a id="mh-elsewhere" href="javascript:mh.nav();">
	  Elsewhere
	</a>
      </div>

      <div id="mh-page">
	<div id="mh-banner">
	  <div id="mh-title">
	    Wonderland
	  </div>
	  <div class="mh-ticket-div" style="right: 130px;">
	    <img class="mh-ticket" src="ticket{{ ticketx|stringformat:"02d" }}.png" />
	    <div class="mh-drink-me-num">{{ ticket }}</div>
	    <div class="mh-drink-me-den">{{ tickett }}</div>
	  </div>
	</div>
	<div id="mh-content">
	  <div style="position: relative;">
	    <img src="map.jpg" />
	  </div>
	  <ul>
	    {% for round in rounds %}
	    {% if round.round.order >= 20 %}
	    <li><a href="{{ root }}/round/{{ round.round.url }}/">{{ round.round.name }}</a></li>
	    {% endif %}
	    {% endfor %}
	  </ul>
	</div>
      </div>
    </div>
{% include "nav.html" %}
    <script src="wonderland.js"></script>
  </body>
</html>
